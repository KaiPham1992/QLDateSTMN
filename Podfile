def main_pods
  pod 'SwiftyJSON'
  pod 'Alamofire'
  pod 'ObjectMapper'
  pod 'BarcodeScanner'

  #Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
end

target 'BaseIOS2018' do
  use_frameworks!
  main_pods
end
